class LinearSearch{
    public static int LinearSearch(int[] arr,int target){
        for(int i=0;i<arr.length;i++){
            if(arr[i] == target){
                return i;
            }else{
                return -1;
            }
        }
    }
    public static void BubbleSort(int[] arr) {
       int[] arr = {53,56,2,6,7,24,7,8};
        int target = 5;
       LinearSearch(arr,target);
    }
}